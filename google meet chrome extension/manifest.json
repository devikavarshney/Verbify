{
    "name":"Verbify",
    "version":"0.7",
    "description":"A chrome extension for adding extra buttons to Google Meet                                                                                                                                                  ",
    "content_scripts":[                             
        {
            "matches":["https://meet.google.com/*"],
            "js":["contentScript.js"],            
            "css":["contentScript.css"],
            "run_at":"document_end"
        }   
    ],
    "icons":{
        "128":"./images/1053219.png"
    },
    "host_permissions":["https://meet.google.com/*"],
    "manifest_version":3
}